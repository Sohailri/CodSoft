class Array
{
	int[][] createArray()
	{
		int x[][]={ {100,200},{300,400} };
		return x;
	}
}
public class TestArray5
{
	public static void main(String args[])
	{
		int arr[][];
		Array obj=new Array();
		arr= obj.createArray();
		for(int row=0;row<2;row++)
		{
			for(int col=0;col<2;col++)
			{
				System.out.print(arr[row][col]);
				System.out.print("  ");
			}
			System.out.println();
		}
	}
}
