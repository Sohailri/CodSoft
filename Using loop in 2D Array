class Array
{
	void display(int a[][])
	{
		for(int row=0;row<2;row++)
		{
		   for(int col=0;col<2;col++)
		   {
			   System.out.println(a[row][col]);
		   }
		}
	}
}
public class TestArray4
{
	public static void main(String args[])
	{
		int x[][]= { {50,60},{70,80} };
		Array obj=new Array();
		obj.display(x);
	}
}
